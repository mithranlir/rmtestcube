<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dimensions xmlns="http://www.quartetfs.com"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.quartetfs.com ../activepivot-types.xsd">
	
	<dimension name="Underlyings" type="REGULAR">
		<hierarchy name="Underlyings" allMember="true">
			<level name="UnderlierCurrency">
				<comparator pluginKey="Custom">
					<order name="firstObjects">
						<value>EUR</value>
						<value>GBP</value>
						<value>USD</value>
						<value>JPY</value>
					</order>
				</comparator>
			</level>
			<level name="UnderlierType" />
			<level name="UnderlierCode" />
		</hierarchy>
		<hierarchy name="Products" allMember="true" defaultHierarchy="true">
			<level name="ProductType" />
			<level name="ProductName" />
		</hierarchy>
	</dimension>
	
	<!-- The dimension will have a unique hierarchy and a unique level -->
	<dimension name="HostName" type="REGULAR" />
	
	<dimension name="Bookings" type="REGULAR">
		<hierarchy name="Desk" allMember="true">
			<level name="Desk">
				<memberProperties>
					<!-- A custom member property -->
					<property name="nextTradeDate" pluginKey="DESK_NEXT_TRADE_DATE" />
				</memberProperties>
			</level>
			<level name="BookId">
				<memberProperties>
					<!-- A default custom member property that uses com.quartetfs.fwk.impl.CustomProperty -->
					<property name="owner" expression="ownerName" />
				</memberProperties>
			</level>
		</hierarchy>
		<hierarchy name="Status" allMember="true">
			<level name="IsSimulated" />
			<level name="Status" />
		</hierarchy>
	</dimension>
	<dimension name="Time" type="TIME">
		<hierarchy name="HistoricalDates" allMember="false">
			<level name="AsOfDate" type="TIME" formatter="DATE[yyyy-MM-dd]">
				<!-- add comparator -->
				<comparator pluginKey="ReverseOrder" />
			</level>
		</hierarchy>
	
		<!-- Standard time buckets, bucketing performed at insertion -->
		<hierarchy name="TimeBucket" folder="Bucketing" allMember="true">
			<level name="DateBucket">
				<!-- add comparator -->
				<comparator pluginKey="Custom">
					<order name="firstObjects">
						<value>1D</value>
						<value>2D</value>
						<value>3D</value>
						<value>1W</value>
						<value>2W</value>
						<value>3W</value>
						<value>1M</value>
						<value>2M</value>
						<value>3M</value>
						<value>6M</value>
						<value>9M</value>
						<value>1Y</value>
						<value>2Y</value>
						<value>5Y</value>
					</order>
				</comparator>
			</level>
			<!-- The Value Date level is the field Date -->
			<level name="Value Date" property="Date" type="TIME" formatter="DATE[dd-MM-yyyy]">
			</level>
		</hierarchy>
		<!-- Dynamic time buckets, bucketing performed at query time. -->
		<!-- This requires the cooperation of bucketing post processor. -->
		<hierarchy name="TimeBucketDynamic" folder="Bucketing" pluginKey="TIME_BUCKET" factless="false"/>
	</dimension>

	<!-- similar to TimeBucketDynamic... -->

	<dimension name="ReferenceCurrencies"  type="REGULAR">
		<hierarchy name="ReferenceCurrencies" pluginKey="REF_CCY" factless="false"/>
	</dimension>

</dimensions>